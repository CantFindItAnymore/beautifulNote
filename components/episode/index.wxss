.container {
  display: inline-flex;
  flex-direction: row;
  height: 60rpx;
}

.indexContainer {
  display: inline-flex;
  flex-direction: row;
  align-items: baseline;
}

.num {
  font-size: 28rpx;
}

.index {
  font-size: 60rpx;
  line-height: 60rpx;
  margin-right: 14rpx;
}

.line {
  height: 44rpx;
  border-left: 1rpx solid #111;
  margin-right: 14rpx;
}

.dateContainer {
  display: inline-flex;
  flex-direction: column;
  font-size: 20rpx;
  justify-content: flex-end;
}